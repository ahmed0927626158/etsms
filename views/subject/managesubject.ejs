

<!doctype html>
<html lang="en">
 <%- include('../partials/header'); -%>
  <body>
<div class="wrapper">
	<div class="body-overlay"></div>
	 <!-------sidebar--design------------>
     <%- include('../partials/sidebar'); -%>
   <!-------sidebar--design- close-----------
   
   
      <!-------page-content start----------->
   
      <div id="content">
		  <!------top-navbar-start-----------> 
			<%- include('../partials/topnav'); -%>


<section class="content-header">
    <h1>
        <small>
            </small>
    </h1>
    <ol class="breadcrumb">
        
        <li><a><i class="fa fa-user-plus active"></i> Manage Subject</a></li>

    </ol>
</section>

<!-- Main content -->
<section class="content container-fluid">
    <!-- /.box-header -->

    <div class="row">

        <div class="col-sm-12 col-xs-12">
            <div class="nav-tabs-custom">
                <ul class="nav nav-tabs">
                    <li class="nav-item"><a class="active nav-link" href="#subject_home" data-toggle="tab" style="padding-right: 15px;">Add Subject</a></li>
                    <li class="nav-item"><a class="nav-link" href="#assign_teacher" data-toggle="tab">Assign Teacher</a></li>
                </ul>
                <div class="tab-content">
                    <div class="active tab-pane" id="subject_home">
                        <!-- Post -->
                        <div class="row">
                            <div class="col-sm-6">
                                <div id="add_subject_box">
                                    <%- include('addSubject'); -%>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="list_subject">
                                    <h4>Subject Information</h4>
                                    <table id="subject_info_table" class="table table-striped" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Title</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for (var i = 0; i < subjects.length; i++) { %>
                                              <tr>
                                                <td><%= subjects[i].id %></td>
                                                <td><%= subjects[i].title %></td>
                                                <td>
                                                  <a href="#">
                                                    <i class="material-icons"> edit</i>
                                                  </a>
                                                  <a href="#">
                                                    <i class="material-icons">delete</i>
                                                  </a>
                                               
                                                </td>
                              
                                              
                                            <% } %>
                                              
                                          </tbody>
                                          <tfoot>
                                              <tr>
                                                <th>#</th>
                                                <th>Title</th>
                                                
                                              </tr>
                                          </tfoot>
                                      </table>
                                </div>
                            </div>
                        </div>
                       
                        <!-- /.post -->

                    </div>

                    <!-- /.tab-pane -->
                    <div class="tab-pane" id="assign_teacher">
                          <div class="row">
                            <div class="col-sm-6">
                                 <div id="assign_subject_teacher_box">
                                    <%- include('assign_subject_teacher'); -%>
                                 </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="list_subject1">
                                    <h4>Subject Teacher Information</h4>
                                    <table id="teacher_subject_grade_info_table" class="table table-striped" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Teacher</th>
                                                <th>Grade</th>
                                                <th>Section</th>
                                                <th>Subject</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for (var i = 0; i < data.length; i++) { %>
                                              <tr>
                                                <td><%= data[i].id %></td>
                                                <td><%= data[i].firstname +" " + data[i].middlename+" " + data[i].lastname %></td>
                                                <td><%= data[i].grade %></td>
                                                <td><%= data[i].section %></td>
                                                <td><%= data[i].subject %></td>
                                                <td>
                                                  <a href="#">
                                                    <i class="material-icons"> edit</i>
                                                  </a>
                                                  <a href="#">
                                                    <i class="material-icons">delete</i>
                                                  </a>
                                               
                                                </td>
                              
                                              
                                            <% } %>
                                              
                                          </tbody>
                                          <tfoot>
                                              <tr>
                                                <th>#</th>
                                                <th>Teacher</th>
                                                <th>Grade<th>
                                                <th>Section</th>
                                                <th>Subject</th>
                                              </tr>
                                          </tfoot>
                                      </table>
                                </div>
                            </div>
                        </div>
  
                    </div>
             
              

                </div>
                <!-- /.tab-content -->
            </div>
        </div>

        </div>

    </div>
    
</section>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
<script type="text/javascript">

</script>
<script type="text/javascript">


  
    </script>
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    
   <script src="js/jquery-3.3.1.slim.min.js"></script>
   <script src="js/popper.min.js"></script>
   <script src="js/bootstrap.min.js"></script>
   <script src="js/jquery-3.3.1.min.js"></script>
  
  
  <script type="text/javascript">
       $(document).ready(function(){
        $('#subject_info_table').DataTable();
        $('#teacher_subject_grade_info_table').DataTable();
	      $(".xp-menubar").on('click',function(){
		    $("#sidebar").toggleClass('active');
			$("#content").toggleClass('active');
		  });
		  
		  $('.xp-menubar,.body-overlay').on('click',function(){
		     $("#sidebar,.body-overlay").toggleClass('show-nav');
		  });
	   });
  </script>
  </body>
  
  </html>








    